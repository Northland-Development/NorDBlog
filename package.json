        "runClient": "cd client && npm run start",
        "runDB": "docker-compose up",
        "start": "node runClientApi.js"
    },
    "dependencies": {
        "concurrently": "^5.3.0",
        "dotenv": "^8.2.0",
        "react-app-env": "^1.2.3"
    }
}
